# Loading project Control
add wave -position insertpoint  \
vsim:/tb/CLK \
vsim:/tb/reset \
vsim:/tb/reset_done \
vsim:/tb/alu_out
add wave -position insertpoint vsim:/tb/MyMips/reg_file/*
add wave -position insertpoint  \
vsim1:/tb/MyMips/DMem/q
add wave -position insertpoint  \
vsim1:/tb/MyMips/DMem/clk \
vsim1:/tb/MyMips/DMem/addr \
vsim1:/tb/MyMips/DMem/data \
vsim1:/tb/MyMips/DMem/we \
vsim1:/tb/MyMips/DMem/q
add wave -position insertpoint vsim1:/tb/P_DUMP_STATE/*
add wave -position insertpoint vsim1:/tb/MyMips/reg_file/*
